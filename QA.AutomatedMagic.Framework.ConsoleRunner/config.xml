    <TestStepGroup name="PrepareGitHubTutorial" description="Prepare GitHubTutorial for tests">
      <order>PrePre</order>
      <Steps>

        <TestStep name="GenerateTutorial" description="Generate tutorial to local folder">
          <manager>GitHubTutorialManager</manager>
          <command>GenerateTutorial</command>
          <args>
            <arg type="Context">GitHubTutorial.Tutorial</arg>
          </args>
        </TestStep>
        <TestStep name="CloneGitRepository" description="Clone Git repository">
          <manager>GitManager</manager>
          <command>Clone</command>
          <args>
            <arg type="Context">GitRepositoryConfig.TutorialRepository</arg>
          </args>
        </TestStep>
        <TestStep name="CopyTutorialToGitRepository" description="Copy tutorial from local folder to local git repository">
          <manager>GitHubTutorialManager</manager>
          <command>CopyToGitRepository</command>
          <args>
            <arg type="Context">GitHubTutorial.Tutorial</arg>
            <arg type="Context">GitRepositoryConfig.TutorialRepository</arg>
          </args>
        </TestStep>
        <TestStep name="UpdateRemoteRepo" description="Update remote Git repository">
          <manager>GitManager</manager>
          <command>CommitAndPushAll</command>
          <args>
            <arg type="Context">GitRepositoryConfig.TutorialRepository</arg>
            <arg type="Value">Commit git hub tutorial</arg>
          </args>
        </TestStep>

      </Steps>
    </TestStepGroup>

    <TestStepGroup name="PrepareAemTutorialPage" description="Prepare Tutorial page for tests">
      <StepMarks>
        <StepMark>PreparePage</StepMark>
      </StepMarks>
      <order>PrePre</order>
      <Steps>

        <TestStep name="CreateAemTutorialPage" description="Create Tutorial page on Author">
          <manager>AemPageManager</manager>
          <command>CreatePage</command>
          <args>
            <arg type="Manager">ApiManager</arg>
            <arg type="Context">AemPage.TutorialPage</arg>
            <arg type="Context">LandscapeConfig.LandscapeConfig</arg>
            <arg type="Context">AemUser.AutotestUser</arg>
          </args>
        </TestStep>

        <TestStepGroup name="SetUpTutorialPage" description="SetUp created Tutorial page">
          <Steps>
            <TestStep name="OpenCreatedPageOnAuthor" description="Open created tutorial page on Author">
              <manager>AemPageManager</manager>
              <command>OpenPageOnAuthor</command>
              <args>
                <arg type="Manager">WebDriverManager</arg>
                <arg type="Context">AemPage.TutorialPage</arg>
                <arg type="Context">LandscapeConfig.LandscapeConfig</arg>
              </args>
            </TestStep>
            <TestStep name="Login" description="Login to Aem">
              <manager>LoginPageManager</manager>
              <command>Login</command>
              <args>
                <arg type="Manager">WebDriverManager</arg>
                <arg type="Context">AemUser.AutotestUser</arg>
              </args>
            </TestStep>
            <TestStep name="SetUpTutorialCatalogComponent" description="Setup TutorialCatalog component">
              <manager>TutorialCatalogPageManager</manager>
              <command>SetUpTutorialCatalog</command>
              <args>
                <arg type="Manager">WebDriverManager</arg>
                <arg type="Context">TutorialCatalogComponentConfig.TutorialCatalogComponentConfig</arg>
              </args>
            </TestStep>
            <TestStep name="AddContainerFinder" description="Add Container finder component to Tutorial Catalog page">
              <manager>TutorialCatalogPageManager</manager>
              <command>AddContainerFinderComponent</command>
              <order>CasePre</order>
              <args>
                <arg type="Manager">WebDriverManager</arg>
                <arg type="Manager">InsertNewComponentFormManager</arg>
              </args>
            </TestStep>
            <TestStep name="SetUpContainerFinder" description="Setup ContainerFinder component">
              <manager>TutorialCatalogPageManager</manager>
              <command>SetUpContainerFinder</command>
              <args>
                <arg type="Manager">WebDriverManager</arg>
                <arg type="Context">ContainerFinderComponentConfig.ContainerFinderComponentConfig</arg>
              </args>
            </TestStep>
            <TestStep name="AddFinderResults" description="Add Finder results component to Tutorial Catalog page">
              <manager>TutorialCatalogPageManager</manager>
              <command>AddFinderResultsComponent</command>
              <args>
                <arg type="Manager">WebDriverManager</arg>
                <arg type="Manager">InsertNewComponentFormManager</arg>
              </args>
            </TestStep>
            <TestStep name="SetUpFinderResults" description="Setup FinderResults component">
              <manager>TutorialCatalogPageManager</manager>
              <command>SetUpFinderResults</command>
              <args>
                <arg type="Manager">WebDriverManager</arg>
                <arg type="Context">FinderResultsComponentConfig.FinderResultsComponentConfig</arg>
              </args>
            </TestStep>
            <TestStep name="AddFacets" description="Add Facets component to Tutorial Catalog page">
              <manager>TutorialCatalogPageManager</manager>
              <command>AddFacetsComponent</command>
              <args>
                <arg type="Manager">WebDriverManager</arg>
                <arg type="Manager">InsertNewComponentFormManager</arg>
              </args>
            </TestStep>
            <TestStep name="SetUpFacets" description="Setup Facets component">
              <manager>TutorialCatalogPageManager</manager>
              <command>SetUpFacets</command>
              <args>
                <arg type="Manager">WebDriverManager</arg>
                <arg type="Context">FacetsComponentConfig.FacetsComponentConfig</arg>
              </args>
            </TestStep>
          </Steps>
        </TestStepGroup>

        <TestStepGroup name="ActivateTutorialCatalogPage" description="Activate created tutorial catalog page">
          <order>PrePre</order>
          <Steps>
            <TestStep name="ActivateTutorialCatalogPage" description="Activate created Tutorial catalog page">
              <Manager>AemPageManager</Manager>
              <Command>ActivatePage</Command>
              <args>
                <arg type="Manager">ApiManager</arg>
                <arg type="Context">AemPage.TutorialPage</arg>
                <arg type="Context">LandscapeConfig.LandscapeConfig</arg>
                <arg type="Context">AemUser.AutotestUser</arg>
              </args>
            </TestStep>
            <TestStep name="WaitForPageActivation" description="Wait until created Tutorial catalog page being activated">
              <Manager>AemPageManager</Manager>
              <Command>WaitForPageActivation</Command>
              <args>
                <arg type="Manager">ApiManager</arg>
                <arg type="Context">AemPage.TutorialPage</arg>
                <arg type="Context">LandscapeConfig.LandscapeConfig</arg>
                <arg type="Context">AemUser.AutotestUser</arg>
              </args>
            </TestStep>
          </Steps>
        </TestStepGroup>

      </Steps>
    </TestStepGroup>

    <TestStep name="RunGitHubJob" description="Run GitHub job for tutorial export">
      <Manager>JobManager</Manager>
      <Command>RunGitHubJob</Command>
      <Order>PrePre</Order>
      <args>
        <arg type="Manager">ApiManager</arg>
        <arg type="Context">LandscapeConfig.LandscapeConfig</arg>
        <arg type="Context">AemUser.AutotestUser</arg>
      </args>
    </TestStep>

    <TestStepGroup name="VeifyChildrenPagesActivation" description="Verify that children page have staus ACTIVATE and wait until their activation completed">
      <Order>PrePre</Order>
      <Steps>
        <TestStep name="VerifyThatChildrenActivated" description="Veirfy that all children Tutorial page are Activated">
          <Manager>AemPageManager</Manager>
          <Command>VerifyChildrenPagesStatus</Command>
          <args>
            <arg type="Manager">ApiManager</arg>
            <arg type="Context">AemPage.TutorialPage</arg>
            <arg type="Context">LandscapeConfig.LandscapeConfig</arg>
            <arg type="Context">AemUser.AutotestUser</arg>
            <arg type="Value">ACTIVATE</arg>
          </args>
        </TestStep>
        <TestStep name="VerifyThatChildrenActivated" description="Veirfy that all children Tutorial page are Activated">
          <Manager>AemPageManager</Manager>
          <Command>WaitForChildPagesActivation</Command>
          <args>
            <arg type="Manager">ApiManager</arg>
            <arg type="Context">AemPage.TutorialPage</arg>
            <arg type="Context">LandscapeConfig.LandscapeConfig</arg>
            <arg type="Context">AemUser.AutotestUser</arg>
          </args>
        </TestStep>
      </Steps>
    </TestStepGroup>

    <TestStepGroup name="GetAndVerifyIssues" description="Get issues and verify with tutorial md files">
      <StepMarks>
        <StepMark>VerifyIssues</StepMark>
      </StepMarks>
      <Order>CasePost</Order>
      <Steps>
        <TestStep name="GetGitHubIssues" description="Get GitHub issues for tutroial repository">
          <Manager>GitManager</Manager>
          <Command>GetIssues</Command>
          <args>
            <arg type="Context">GitRepositoryConfig.TutorialRepository</arg>
          </args>
        </TestStep>
        <TestStep name="VerifyTutorialIssues" description="Verify issues for turotial md files">
          <Manager>GitHubTutorialManager</Manager>
          <Command>VerifyTutorialIssues</Command>
          <args>
            <arg type="Context">GitHubTutorial.Tutorial</arg>
            <arg type="StepResult">GetAndVeifyIssues.GetGitHubIssues</arg>
          </args>
        </TestStep>
      </Steps>
    </TestStepGroup>

    <TestStepGroup name="GetAndVerifyTutorialCardsOnAuthor" description="Get cards and verify with tutorial md files on Author">
      <StepMarks>
        <StepMark>VerifyCardsOnAuthor</StepMark>
      </StepMarks>
      <Order>CasePost</Order>
      <Steps>
        <TestStep name="GetTutorialCardsFromTutorialPage" description="Get GitHub tutorial cards from TutorialCatalogPage on Author">
          <Manager>TutorialCatalogPageManager</Manager>
          <Command>GetTutorialCardsOnAuthor</Command>
          <args>
            <arg type="Manager">WebDriverManager</arg>
          </args>
        </TestStep>
        <TestStep name="VerifyTutorialIssues" description="Verify issues for turotial md files">
          <Manager>GitHubTutorialManager</Manager>
          <Command>VerifyTutorialCards</Command>
          <args>
            <arg type="Context">GitHubTutorial.Tutorial</arg>
            <arg type="StepResult">GetAndVerifyTutorialCardsOnAuthor.GetTutorialCardsFromTutorialPage</arg>
          </args>
        </TestStep>
      </Steps>
    </TestStepGroup>

    <TestStepGroup name="GetAndVerifyTutorialCardsOnPublish" description="Get cards and verify with tutorial md files on Author">
      <StepMarks>
        <StepMark>VerifyCardsOnPublish</StepMark>
      </StepMarks>
      <Order>CasePost</Order>
      <Steps>
        <TestStep name="OpenCreatedPageOnPubslih" description="Open created tutorial page on Pubslih">
          <manager>AemPageManager</manager>
          <command>OpenPageOnPubslih</command>
          <args>
            <arg type="Manager">WebDriverManager</arg>
            <arg type="Context">AemPage.TutorialPage</arg>
            <arg type="Context">LandscapeConfig.LandscapeConfig</arg>
          </args>
        </TestStep>
        <TestStep name="GetTutorialCardsFromTutorialPage" description="Get GitHub tutorial cards from TutorialCatalogPage on Author">
          <Manager>TutorialCatalogPageManager</Manager>
          <Command>GetTutorialCardsOnPubslih</Command>
          <args>
            <arg type="Manager">WebDriverManager</arg>
          </args>
        </TestStep>
        <TestStep name="VerifyTutorialIssues" description="Verify issues for turotial md files">
          <Manager>GitHubTutorialManager</Manager>
          <Command>VerifyTutorialCards</Command>
          <args>
            <arg type="Context">GitHubTutorial.Tutorial</arg>
            <arg type="StepResult">GetAndVerifyTutorialCardsOnAuthor.GetTutorialCardsFromTutorialPage</arg>
          </args>
        </TestStep>
      </Steps>
    </TestStepGroup>

  </Steps>

  <Suites>

    <TestSuite name="CreateUpdateTutorial" description="Create/Update new tutorial">

      <context>
        <contextItems>

          <ContextObjectItem name="GitRepositories configs">
            <objects>
              <GitRepositoryConfig uniqueName="TutorialRepository">
                <RemoteRepository>https://github.com/ksAutotests/CreateUpdateTutorial</RemoteRepository>
                <LocalRepository>c:\tmp\localgitrepo\CreateUpdateTutorial</LocalRepository>
              </GitRepositoryConfig>
            </objects>
          </ContextObjectItem>

          <DynamicContextObjectItem name="AemPages configs">
            <DynamicObjects>
              <AemPage uniqueName="TutorialPage">
                <title>
                  <DynamicNode>
                    <Node>
                      <ComplexNode>
                        <Nodes>
                          <ValueNode>autotest-Tutorial-</ValueNode>
                          <GenerationNode>
                            <Generator>
                              <StringGenerator></StringGenerator>
                            </Generator>
                          </GenerationNode>
                        </Nodes>
                      </ComplexNode>
                    </Node>
                  </DynamicNode>
                </title>
                <parentPath>/content/sapdx/website/languages/en/developer</parentPath>
                <template>/apps/sapdx/templates/tutorial/tutorialPage</template>
              </AemPage>
            </DynamicObjects>
          </DynamicContextObjectItem>

          <DynamicContextObjectItem name="Dynamic Component configs">
            <DynamicObjects>

              <TutorialCatalogComponentConfig uniqueName="TutorialCatalogComponentConfig">
                <TutorialCardPath>
                  <DynamicNode>
                    <Node>
                      <ContextValueNode value="GitRepositoryConfig.TutorialRepository.RemoteRepository"></ContextValueNode>
                    </Node>
                  </DynamicNode>
                </TutorialCardPath>
                <HideFacetsWithoutResults>True</HideFacetsWithoutResults>
                <ExternalSource>True</ExternalSource>
              </TutorialCatalogComponentConfig>

              <ContainerFinderComponentConfig uniqueName="ContainerFinderComponentConfig">
                <PagePaths>
                  <path>
                    <DynamicNode>
                      <Node>
                        <ContextValueNode value="AemPage.TutorialPage.Path"></ContextValueNode>
                      </Node>
                    </DynamicNode>
                  </path>
                </PagePaths>
              </ContainerFinderComponentConfig>

            </DynamicObjects>
          </DynamicContextObjectItem>

          <ContextObjectItem name="Components configs">
            <objects>

              <FinderResultsComponentConfig uniqueName="FinderResultsComponentConfig">
                <Pagination>5</Pagination>
                <!--<DefaultDocumentIcon>/content/dam/application/shared/icons/finder</DefaultDocumentIcon>
                <DefaultPageIcon>/content/dam/application/shared/icons/icon_gold_document_nopadding.svg</DefaultPageIcon>
                <DefaultVideotIcon>/content/dam/application/shared/icons/finder</DefaultVideotIcon>-->

                <AlphabeticalSorting>Both</AlphabeticalSorting>
                <SortingByDate>Both</SortingByDate>
                <DefaultSorting>Newest</DefaultSorting>

                <ResultView>Grid</ResultView>
                <ShowDescription>True</ShowDescription>
                <ShowTags>True</ShowTags>
                <ShowUpdateDate>True</ShowUpdateDate>
              </FinderResultsComponentConfig>

              <FacetsComponentConfig uniqueName="FacetsComponentConfig">
                <Namespaces>
                  <namespace>/etc/tags/tutorial/interest</namespace>
                  <namespace>/etc/tags/tutorial/product</namespace>
                  <namespace>/etc/tags/tutorial/technology</namespace>
                </Namespaces>
                <HideFacets>True</HideFacets>
                <TypeOfSelection>CheckBoxes</TypeOfSelection>
              </FacetsComponentConfig>

            </objects>
          </ContextObjectItem>

        </contextItems>
      </context>

      <Suites>

