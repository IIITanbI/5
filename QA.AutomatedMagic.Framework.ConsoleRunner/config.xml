                            <Description>SimpleTutorial2 Description</Description>
                            <Tags>
                              <tag>
                                <tag>tutorials:technology/github</tag>
                              </tag>
                              <tag>
                                <tag>tutorials:interest/bigData</tag>
                              </tag>
                            </Tags>
                            <Content>SimpleTutorial2 Content</Content>
